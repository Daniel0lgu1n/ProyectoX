<div class="container">
  <% @json.each do |va1| %>
    <div class="row">
      <% va1.each do |va| %>
      <a href="watch?v=<%= va.split("\;")[0].split("www.xvideos.com/video").last.split("/").first %>_<%= va.split("\;")[0].split("www.xvideos.com/video").last.split("/").last %>">
        <div class="col-sm-2 mx-2">
          <label><%= va.split("\;")[1] %></label>
          <img src="<%= va.split("\;")[3] %>" width="130px" height="100px" class="img-rounded" alt="<%= va.split("\;")[1] %>">
          <label>duracion: <%= va.split("\;")[2] %></label>
        </div>
      </a>
      <% end %>
    </div>
  <% end %>
</div>

