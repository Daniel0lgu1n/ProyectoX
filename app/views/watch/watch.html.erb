<div class="container">
	<div class="row">
		<h2><%= @info_video_json.split("\;")[1] %></h2>
	</div>
	<div class="row">
	  	<div class="col-sm-10 mx-9">
			<iframe 
				width="800" 
				height="400" 
				src="https://www.xvideos.com/embedframe/<%= @codigo %>">
			</iframe>
		</div>
		<div class="col-sm-2 mx-3">
		  	<label>anuncio</label>
		</div>
	</div>
	<div class="row">
	  	<div class="col-sm-10 mx-10">
			Titulo: <%= @info_video_json.split("\;")[1] %>
			<br>
			Duracion: <%= @info_video_json.split("\;")[2] %>
			<br>
			Imagen video: <%= @info_video_json.split("\;")[3] %>
			<br>
			Codigo iframe: <%= @info_video_json.split("\;")[4] %>
			<br>
			Etiquetas: <%= @info_video_json.split("\;")[5] %>
			<br>
			Calidad de video: <%= @info_video_json.split("\;")[9] %>
			<br>
			Tipo de video: <%= @info_video_json.split("\;")[8] %>
			<br>
		</div>
		<div class="col-sm-2 mx-2">
		  	<label>anuncio</label>
		</div>
	</div>
</div>

